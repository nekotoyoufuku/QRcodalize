{
  "expo": {
    "name": "QRcodalize",
    "slug": "QRcodalize",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amagin.QRcodalize",
      "infoPlist": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Images",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": [
              "public.image"
            ],
            "NSDocumentDirectoryUsageDescription": "This app needs access to the document directory to save assets."
          }
        ],
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to allow you to share images within the app."
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "C617.1"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.amagin.QRcodalize"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SpaceMono-Regular.ttf"
          ]
        }
      ],
      [
        "expo-share-extension",
        {
          "height": 500,
          "activationRules": [
            {
              "type": "image",
              "max": 2
            }
          ],
          "excludedPackages": [
            "expo-dev-client",
            "expo-splash-screen",
            "expo-updates",
            "expo-font"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "QRcodalizeShareExtension",
                  "bundleIdentifier": "com.amagin.QRcodalize.ShareExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.amagin.QRcodalize"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "2f99ba06-f289-4ad4-a0cc-733bb667b126"
      },
      "appleApplicationGroup": "group.com.amagin.QRcodalize"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "nekotoyoufuku",
    "updates": {
      "url": "https://u.expo.dev/2f99ba06-f289-4ad4-a0cc-733bb667b126"
    }
  }
}
